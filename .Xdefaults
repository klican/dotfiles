! -- General -- !
URxvt*jumpScroll:               True
URxvt*depth:                    32
!URxvt*transparent:              True
!URxvt*cursorColor:              grey
URxvt*loginShell:               False
URxvt*visualBell:               False
URxvt*scrollBar:                False
URxvt*scrollTtyOutput:          False
URxvt*scrollTtyKeypress:        True
URxvt*scrollWithBuffer:         True
URxvt*cursorBlink:              False
URxvt*saveLines:                1000
URxvt*internalBorder:           0
URxvt*borderLess:               False
URxvt*termName:                screen-256color
URxvt*fading:                   0
URxvt*shading:                  100
URxvt*perl-ext:                 matcher
URxvt*urlLauncher:              firefox
!URxvt*background:               rgba:0000/0000/0000/aaaa

! -- xft config -- !
Xft.lcdfilter: lcddefault
Xft.rgba: rgb
Xft.autohint: 0
Xft.hinting: 1
Xft.hintstyle: hintfull
Xft.antialias: 1
Emacs.font: Envy Code R for Powerline-10

! -- Use the Envy Code R font, and work properly with vim-powerline: -- !
URxvt.font:       xft:Envy Code R for Powerline:size=10:antialias=true
URxvt.boldFont:   xft:Envy Code R for Powerline:bold:size=10:antialias=true
URxvt.italicFont: xft:Envy Code R for Powerline:italic:size=10:antialias=true

