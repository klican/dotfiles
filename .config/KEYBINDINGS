    <keybind key="A-p">
      <action name="Execute">
        <execute>/home/lelio/bin/dmenu_launch.sh</execute>

 
    <keybind key="C-A-S-Delete">
      <action name="Execute">
	      <execute>/sbin/halt</execute>


    <keybind key="C-A-S-r">
      <action name="Execute">
	      <execute>/sbin/reboot</execute>



    <keybind key="C-A-u">
      <action name="Execute">
        <command>~/bin/urxvtc</command>



    <keybind key="C-A-r">
      <action name="Execute">
        <startupnotify>
          <enabled>true</enabled>
          <name>Ranger</name>
        </startupnotify>
        <decor>no</decor>
        <command>urxvt  -name urxvt-nodecor --geometry 115x22 -e ranger</command>



