    <keybind key="W-p">
      <action name="Execute">
        <startupnotify>
          <enabled>true</enabled>
          <name>lastsong</name>
        </startupnotify>
        <command>mpc prev</command>
      </action>
    </keybind>
    <keybind key="W-n">
      <action name="Execute">
        <startupnotify>
          <enabled>true</enabled>
          <name>nextsong</name>
        </startupnotify>
        <command>mpc next</command>
      </action>
    </keybind>
    <keybind key="W-Return">
      <action name="Execute">
        <startupnotify>
          <enabled>true</enabled>
          <name>playorpausesong</name>
        </startupnotify>
        <command>mpc toggle</command>
      </action>
    </keybind>
    <keybind key="W-k">
      <action name="Execute">
        <command>~/.scripts/VolumeNotifyPCM.sh up</command>
      </action>
    </keybind>
    <keybind key="W-j">
      <action name="Execute">
        <command>~/.scripts/VolumeNotifyPCM.sh down</command>
      </action>
    </keybind>
    <keybind key="XF86AudioRaiseVolume">
      <action name="Execute">
        <command>~/.scripts/VolumeNotifyMaster.sh up</command>
      </action>
    </keybind>
    <keybind key="XF86AudioLowerVolume">
      <action name="Execute">
        <command>~/.scripts/VolumeNotifyMaster.sh down</command>
      </action>
    </keybind>
    <keybind key="XF86AudioMute">
      <action name="Execute">
        <command>~/.scripts/VolumeNotifyMaster.sh toggle</command>
      </action>
    </keybind>
